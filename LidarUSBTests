import serial
import threading

port = '/dev/ttyUSB0'
baud = 115200
serial_port = serial.Serial(port, baud, timeout=0)

ser = serial.Serial()
ser.port = '/dev/ttyUSB0'
ser.baudrate = 115200
ser.parity = serial.PARITY_NONE
ser.stopbits = serial.STOPBITS_ONE
ser.bytesize = serial.EIGHTBITS
ser.timeout = 3
print(ser)
ser.open()
ser.write(b'T')
ser.read(5)
ser.close()
